{
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "ui": {
    "theme": "GitHub",
    "showStatusInTitle": true,
    "showLineNumbers": false
  },
  "general": {
    "vimMode": true,
    "previewFeatures": true,
    "enablePromptCompletion": true
  },
  "tools": {
    "shell": {
      "showColor": true
    }
  },
  "mcpServers": {
    "dash-api": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/Kapeli/dash-mcp-server.git",
        "dash-mcp-server"
      ]
    },
    "playwright": {
      "type": "stdio",
      "command": "mcp-server-playwright",
      "args": [],
      "env": {}
    },
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "{{ onepasswordRead "op://Personal/GitHub/API Keys/2cnp2x7mnbocwfk6hztik6drxa" }}"
      }
    },
    "prefect": {
      "command": "uvx",
      "args": ["--from", "prefect-mcp", "prefect-mcp-server"]
    },
    "notion": {
      "type": "sse",
      "url": "https://mcp.notion.com/sse"
    },
    "exa": {
      "type": "http",
      "url": "https://mcp.exa.ai/mcp",
      "headers": {}
    },
    "linear": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.linear.app/mcp"]
    },
    "emacs": {
      "command": "uv",
      "args": [
        "--directory",
        "/Users/jesse/src/github.com/jesse-c/emacs-mcp-server",
        "run",
        "emacs-mcp-server"
      ]
    }
  }
}
